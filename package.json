{
  "name": "@byyuurin/ui",
  "type": "module",
  "version": "0.0.11",
  "packageManager": "pnpm@10.17.1",
  "description": "",
  "author": "Yuurin <byyuurin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/byyuurin/ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byyuurin/ui.git"
  },
  "bugs": "https://github.com/byyuurin/ui/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/runtime/index.d.ts",
      "default": "./dist/runtime/index.js"
    },
    "./unplugin": {
      "types": "./dist/unplugin.d.mts",
      "default": "./dist/unplugin.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.mts",
      "default": "./dist/vite.mjs"
    },
    "./nuxt": {
      "types": "./dist/module.d.mts",
      "default": "./dist/module.mjs"
    },
    "./unocss": {
      "types": "./dist/unocss.d.mts",
      "default": "./dist/unocss.mjs"
    },
    "./runtime/index": {
      "types": "./dist/runtime/index.d.ts",
      "default": "./dist/runtime/index.js"
    },
    "./runtime/*": "./dist/runtime/*",
    "./utils/*": {
      "types": "./dist/runtime/utils/*.d.ts",
      "default": "./dist/runtime/utils/*.js"
    },
    "./locale": {
      "types": "./dist/runtime/locale/index.d.ts",
      "default": "./dist/runtime/locale/index.js"
    }
  },
  "main": "dist/runtime/index.js",
  "module": "dist/runtime/index.js",
  "types": "dist/runtime/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground/nuxt",
    "dev:build": "nuxi build playground/nuxt",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground/nuxt",
    "dev:vue": "pnpm --filter \"./playground/vue\" run dev",
    "lint": "eslint .",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:types": "vue-tsc --noEmit && nuxi typecheck playground/nuxt",
    "prepare": "simple-git-hooks",
    "prepack": "nuxt-module-build build"
  },
  "peerDependencies": {
    "@unocss/core": ">=66.1.0",
    "@unocss/preset-wind3": ">=66.1.0"
  },
  "dependencies": {
    "@byyuurin/ui-kit": "^0.7.0",
    "@byyuurin/uno-merge": "^0.2.0",
    "@internationalized/date": "^3.9.0",
    "@nuxt/kit": "^3.19.2",
    "@standard-schema/spec": "^1.0.0",
    "@tanstack/vue-table": "^8.21.3",
    "@unocss/preset-mini": "^66.5.2",
    "@vue/shared": "^3.5.22",
    "@vueuse/core": "^13.9.0",
    "defu": "^6.1.4",
    "embla-carousel": "^8.6.0",
    "embla-carousel-auto-height": "^8.6.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-class-names": "^8.6.0",
    "embla-carousel-fade": "^8.6.0",
    "embla-carousel-vue": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0",
    "magic-string": "^0.30.19",
    "mlly": "^1.8.0",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "reka-ui": "^2.5.0",
    "scule": "^1.3.0",
    "tinyglobby": "^0.2.15",
    "ufo": "^1.6.1",
    "unplugin": "^2.3.10",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vue-component-type-helpers": "^2.2.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/ni": "^25.0.0",
    "@byyuurin/eslint-config": "^1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.19.2",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^22.18.6",
    "@unocss/core": "^66.5.2",
    "@unocss/preset-wind3": "^66.5.2",
    "@unocss/reset": "^66.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "1.0.1",
    "esno": "^4.8.0",
    "lint-staged": "^16.2.0",
    "nuxt": "^3.19.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.2",
    "unocss": "^66.5.2",
    "vite": "^7.1.7",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-tsc": "^2.2.12",
    "zod": "^3.25.76"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "isarray": "npm:@nolyfill/isarray@^1.0.44",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1.0.44"
    }
  },
  "resolutions": {
    "@byyuurin/ui": "workspace:*"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
