{
  "name": "@byyuurin/ui",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.4",
  "description": "",
  "author": "Yuurin <byyuurin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/byyuurin/ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byyuurin/ui.git"
  },
  "bugs": "https://github.com/byyuurin/ui/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs"
    },
    "./unocss-preset": {
      "types": "./dist/unocss-preset.d.ts",
      "import": "./dist/unocss-preset.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev:prepare": "pnpm --filter \"./playground/*\" run dev:prepare",
    "dev:vue": "pnpm --filter \"./playground/vue\" run dev",
    "dev:nuxt": "pnpm --filter \"./playground/nuxt\" run dev",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks && pnpm run stub"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@byyuurin/ui-kit": "^0.3.2",
    "@unocss/core": "^65.4.2",
    "@unocss/preset-mini": "^65.4.2",
    "@vueuse/core": "^12.4.0",
    "defu": "^6.1.4",
    "reka-ui": "1.0.0-alpha.8"
  },
  "devDependencies": {
    "@antfu/ni": "^23.2.0",
    "@byyuurin/eslint-config": "^1.7.0",
    "@nuxt/kit": "^3.15.2",
    "@nuxt/schema": "^3.15.2",
    "@types/node": "^22.10.7",
    "bumpp": "^9.10.1",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "1.0.1",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "unplugin-vue-components": "^28.0.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "resolutions": {
    "@byyuurin/ui": "workspace:*",
    "mkdist": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
